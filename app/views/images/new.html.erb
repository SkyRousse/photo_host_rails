<h1>Upload a New Image</h1>

<%= form_for [current_user, @image] do |f|%>
  <!-- <%= token_tag form_authenticity_token %> -->
  <div class="form-group">
      <%= f.label :image, class: 'control-label'  %>
      <div class="col-sm-6">
          <%= f.file_field :image %>
      </div>

      <%= f.submit 'Upload' %>
  </div>

<% end %>

<%= link_to "Back", '/' %>
